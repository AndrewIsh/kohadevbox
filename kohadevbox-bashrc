# Aliases for reading logs
alias koha-opac-err="sudo tail -f /var/log/koha/kohadev/opac-error.log"
alias koha-intra-err="sudo tail -f /var/log/koha/kohadev/intranet-error.log"

# Alias for getting the db/admin password for our instance
alias koha-pass="sudo xmlstarlet sel -t -v 'yazgfs/config/pass' '/etc/koha/sites/kohadev/koha-conf.xml'"
